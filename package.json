{
  "name": "ember-simple-charts",
  "version": "3.0.1",
  "description": "Simple charts for ember.",
  "keywords": [
    "ember-addon",
    "visualizations",
    "d3"
  ],
  "repository": "https://github.com/ilios/ember-simple-charts",
  "homepage": "https://ember-simple-charts.netlify.com/",
  "license": "MIT",
  "author": "The Ilios Team (info@iliosproject.org)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "lint:style": "stylelint addon/styles tests/dummy/app/styles"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.1",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@popperjs/core": "^2.0.3",
    "d3-ease": "^1.0.5",
    "d3-hierarchy": "^1.1.8",
    "d3-interpolate": "^1.3.2",
    "d3-scale": "^3.0.0",
    "d3-scale-chromatic": "^1.3.3",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.3",
    "d3-transition": "^1.2.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-cli-sass": "^10.0.0",
    "ember-concurrency": "^1.1.2",
    "ember-concurrency-decorators": "^2.0.0",
    "ember-did-resize-modifier": "^1.0.0",
    "ember-in-element-polyfill": "^1.0.0",
    "eslint": "^7.0.0",
    "merge": "^1.2.0"
  },
  "devDependencies": {
    "@ember/edition-utils": "^1.1.1",
    "@ember/optional-features": "^1.3.0",
    "@ilios/ember-template-lint-plugin": "^2.0.1",
    "@ilios/eslint-config-ember-addon": "^2.0.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.17.0",
    "ember-cli-addon-docs": "^0.7.0",
    "ember-cli-addon-docs-esdoc": "^0.2.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-percy": "^1.2.22",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.17.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "qunit-dom": "^1.1.0",
    "sass": "^1.13.2",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-scss": "^3.5.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "pre-commit": [
    "lint"
  ]
}
